{
  "name": "react-startup",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": " gulp && webpack --progress --config ./webpack/webpack.prod.js --env.production",
    "build:performance": "gulp && webpack --display-modules --sort-modules-by size --config ./webpack/webpack.prod.js --env.production",
    "dev": "cross-env DEPLOY_ENV=dev _PLATFORM_=h5 gulp --gulpfile gulpfile.dev.js",
    "eslint": "./node_modules/.bin/eslint . --ext .js",
    "eslint:fix": "./node_modules/.bin/eslint . --ext .js --fix",
    "node": "nodemon ./server/bin/dev --watch server --watch config",
    "pm2st": "cross-env DEPLOY_ENV=prd pm2 start ./dist/server/bin/prod -i max --name xXx",
    "pm2rs": "cross-env DEPLOY_ENV=prd pm2 restart xXx",
    "pm2stop": "pm2 stop xXx",
    "pm2rl": "cross-env DEPLOY_ENV=prd pm2 reload xXx",
    "start": "cross-env DEPLOY_ENV=prd node ./dist/server/bin/prod",
    "stylelint": "./node_modules/.bin/stylelint ./shared/pages/**/*.scss --syntax scss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
